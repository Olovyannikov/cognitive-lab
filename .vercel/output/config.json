{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^((?!/api).*)$",
      "dest": "/ssr_/?__original_path=$1",
      "check": true
    }
  ],
  "overrides": {
    "404.html": {
      "path": "404"
    },
    "blog/index.html": {
      "path": "blog"
    },
    "faq/index.html": {
      "path": "faq"
    },
    "free-report/index.html": {
      "path": "free-report"
    },
    "full-report/example/index.html": {
      "path": "full-report/example"
    },
    "help/index.html": {
      "path": "help"
    },
    "index.html": {
      "path": ""
    },
    "payment-check/index.html": {
      "path": "payment-check"
    },
    "test/index.html": {
      "path": "test"
    },
    "types/index.html": {
      "path": "types"
    }
  }
}